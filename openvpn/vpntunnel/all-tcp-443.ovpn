client
dev tun
proto tcp
; Cert
ns-cert-type server
cipher BF-CBC
;Host
resolv-retry infinite
;auth
auth-user-pass /openvpn-firefox/credentials.txt
keepalive 10 30
sndbuf 0
rcvbuf 0

; load-balanced default
remote-random
remote au-vpn.vpntunnel.com 443
remote bn-ar-vpn.vpntunnel.com 443
remote ca-us-vpn.vpntunnel.com 443
remote ca-vpn.vpntunnel.com 443
remote ch-vpn.vpntunnel.com 443
remote cn-vpn.vpntunnel.com 443
remote co-uk-vpn.vpntunnel.com 443
remote cz-vpn.vpntunnel.com 443
remote dk-vpn.vpntunnel.com 443
remote es-vpn.vpntunnel.com 443
remote fi-vpn.vpntunnel.com 443
remote fl-us-vpn.vpntunnel.com 443
remote fr-de-vpn.vpntunnel.com 443
remote in-vpn.vpntunnel.com 443
remote is-ta-vpn.vpntunnel.com 443
remote is-vpn.vpntunnel.com 443
remote lx-vpn.vpntunnel.com 443
remote mr-ca-vpn.vpntunnel.com 443
remote nl-vpn.vpntunnel.com 443
remote pa-vpn.vpntunnel.com 443
remote pr-it-vpn.vpntunnel.com 443
remote pt-vpn.vpntunnel.com 443
remote ru-vpn.vpntunnel.com 443
remote se-vpn.vpntunnel.com 443
remote tr-vpn.vpntunnel.com 443
remote va-us-vpn.vpntunnel.com 443
remote vn-ca-vpn.vpntunnel.com 443
remote vn-it-vpn.vpntunnel.com 443

persist-key
persist-tun
persist-remote-ip
nobind
comp-lzo
verb 2
